<?xml version="1.0" encoding="UTF-8"?>
 
<?import java.net.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>

<fx:root xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" type="javafx.scene.layout.Pane">
	<AnchorPane fx:id="backgroundUI" minWidth="160.0" minHeight="150.0"
		AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" 
		AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
		styleClass="flowRendererSC">
		<children>
			<Label fx:id="avatarUI" scaleY="1.5"
				AnchorPane.topAnchor="25.0" AnchorPane.leftAnchor="10.0">
               	<font>
					<Font name="System Bold Italic" size="55.0" />
            	</font>
            </Label>
			<HBox AnchorPane.topAnchor="10.0" AnchorPane.rightAnchor="10.0" spacing="4.0">
				<children>
					<Label fx:id="symbolUI" underline="true" />
					<Label fx:id="amountUI" />
				</children>
			</HBox>
			<VBox fx:id="listUI" alignment="TOP_RIGHT"
				AnchorPane.topAnchor="110.0" AnchorPane.bottomAnchor="5.0"
				AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="10.0" />
		</children>
	</AnchorPane>
</fx:root>
