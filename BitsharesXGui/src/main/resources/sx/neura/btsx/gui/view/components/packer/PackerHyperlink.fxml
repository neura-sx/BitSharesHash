<?xml version="1.0" encoding="UTF-8"?>
 
<?import java.net.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>

<fx:root type="javafx.scene.layout.Pane" xmlns:fx="http://javafx.com/fxml">
	<AnchorPane styleClass="packerPaneSC"
		AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
		<children>
			<Label fx:id="labelUI"
				AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="4.0"
				styleClass="packerLabelSC" />
			<ImageView fx:id="avatarUI" preserveRatio="true" smooth="true" fitWidth="80.0"
				AnchorPane.topAnchor="-2.0" AnchorPane.bottomAnchor="2.0" AnchorPane.rightAnchor="10.0"/>
			<Label fx:id="textUI" AnchorPane.topAnchor="12.0"
				AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
				styleClass="packerTextSC" onMouseClicked="#onAction" />
		</children>
	</AnchorPane>
</fx:root>
