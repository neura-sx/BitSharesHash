<?xml version="1.0" encoding="UTF-8"?>
 
<?import java.net.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.Pane" xmlns:fx="http://javafx.com/fxml">
	<AnchorPane styleClass="packerPaneSC"
		AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
		<children>
			<Label fx:id="labelUI"
				AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="4.0"
				styleClass="packerLabelSC" />
			<Button text="Select File" focusTraversable="false" onAction="#selectFile"
				AnchorPane.topAnchor="20.0" AnchorPane.leftAnchor="11.0" />
			<TextArea fx:id="textUI" prefHeight="100.0" editable="true" wrapText="true" focusTraversable="false"
				AnchorPane.topAnchor="45.0" AnchorPane.bottomAnchor="8.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
				styleClass="packerTextSC" />
		</children>
	</AnchorPane>
</fx:root>
